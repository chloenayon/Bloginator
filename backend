import os, sqlite3, csv

def go():

    if (not os.path.exists("blogData.db")):

        conn = sqlite3.connect("blogData.db")
        c = conn.cursor()

        q = "create table members (user text, pwd text, id integer)"
        c.execute(q)

        q = "insert into members (user, pwd, id) values ('Theo', 'TheO', 02)"
        c.execute(q)

    conn.commit()

def ex():

    rd = csv.DictReader(open("blogData.db"))
    BASE = "Name : %(user)s"
    for line in rd:
        print BASE%line

go()
ex()
